{
  "name": "slickgrid-colgroup-plugin",
  "version": "0.1.3",
  "description": "SlickGrid plugin to create column group in a header.",
  "main": "dist/slick.colgroup.js",
  "scripts": {
    "install": "bower i",
    "lint": "eslint src/*.es",
    "start:dev": "http-server & watchify src/slick.colgroup.es -o dist/slick.colgroup.js -t babelify -v",
    "test": "mocha-phantomjs tests/index.html",
    "build": "npm run clean && npm run lint && npm run browserify && npm run replace:version && npm run uglify && npm run test",
    "version": "npm run build && git add .",
    "-----": "----------------------------------------------------------------",
    "replace:version": "VERSION=$(node -e 'console.log(require(\"./package.json\").version)') && perl -i -pe 's/\\$VERSION/v'\"$VERSION\"'/' 'dist/slick.colgroup.js'",
    "clean": "rimraf dist && mkdirp dist",
    "browserify": "browserify -t babelify src/slick.colgroup.es > dist/slick.colgroup.js",
    "uglify": "uglifyjs dist/slick.colgroup.js -cm --comments -o dist/slick.colgroup.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keik/slickgrid-colgroup-plugin.git"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "plugin"
  ],
  "author": {
    "name": "keik",
    "email": "k4t0.kei@gmail.com",
    "url": "keik.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keik/slickgrid-colgroup-plugin/issues"
  },
  "homepage": "https://github.com/keik/slickgrid-colgroup-plugin#readme",
  "devDependencies": {
    "babelify": "^6.2.0",
    "bower": "^1.5.3",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "debug": "^2.2.0",
    "eslint": "^1.2.1",
    "estraverse": "^4.1.1",
    "http-server": "^0.8.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "mocha-phantomjs": "^4.0.1",
    "rimraf": "^2.4.2",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  }
}
